# Redirects and headers for GitHub Pages
/assets/*  /assets/:splat  200
/dist/*   /dist/:splat     200

# Headers
/assets/*
  Cache-Control: public, max-age=31536000, immutable

/dist/*
  Cache-Control: public, max-age=31536000, immutable

/styles.css
  Cache-Control: public, max-age=31536000, immutable

/reset.css
  Cache-Control: public, max-age=31536000, immutable

/index.html
  Cache-Control: public, max-age=3600